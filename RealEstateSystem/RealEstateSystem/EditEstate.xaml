<Window x:Class="RealEstateSystem.EditEstate"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="EditEstate" Height="300" Width="400" Loaded="Window_Loaded">
    <Grid>
        <DataGrid AutoGenerateColumns="False" Height="200" HorizontalAlignment="Left" Margin="0,12,0,0" Name="EditEstatee" VerticalAlignment="Top" Width="252" SelectionChanged="EditEstatee_SelectionChanged">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Id" Binding="{Binding Path=Id}"/>
                <DataGridTextColumn Header="Name" Binding="{Binding Path=Name}"/>
                <DataGridTextColumn Header="Fee" Binding="{Binding Path=Fee}"/>
            </DataGrid.Columns>
        </DataGrid>
        <Button Content="Update Estate" Height="23" HorizontalAlignment="Left" Margin="291,12,0,0" Name="update" VerticalAlignment="Top" Width="75" Click="update_Click" />
        <Button Content="Delete Estate" Height="23" HorizontalAlignment="Left" Margin="291,41,0,0" Name="delete" VerticalAlignment="Top" Width="75" Click="delete_Click" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="303,217,0,0" Name="cancel" VerticalAlignment="Top" Width="75" Click="cancel_Click" />
    </Grid>
</Window>

<Window x:Class="RealEstateSystem.UpdateClient"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Update client details" Loaded="Window_Loaded" mc:Ignorable="d" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" d:DesignHeight="152" d:DesignWidth="272" SizeToContent="WidthAndHeight">
    <Grid Height="111" Width="250">
        <Label Content="First Name" Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label1" VerticalAlignment="Top" />
        <Label Content="Last Name" Height="28" HorizontalAlignment="Left" Margin="12,46,0,0" Name="label2" VerticalAlignment="Top" />

        <TextBox Height="23" HorizontalAlignment="Left" Margin="118,17,0,0" Name="FirstName" VerticalAlignment="Top" Width="120" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="118,46,0,0" Name="LastName" VerticalAlignment="Top" Width="120" />
        <Button Content="Update" Height="23" HorizontalAlignment="Left" Margin="12,80,0,0" Name="Update" VerticalAlignment="Top" Width="145" Click="Update_Click" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Right" Margin="0,80,12,0" Name="Cancel" VerticalAlignment="Top" Width="75" Click="Cancel_Click" />
    </Grid>
</Window>

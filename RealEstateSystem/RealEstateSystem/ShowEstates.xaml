<Window x:Class="RealEstateSystem.ShowEstates"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ShowEstates" Height="300" Width="400">
    <Grid>
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="12,12,0,0" Name="textBlock1" Text="Show estates where fee is greater then " VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="225,9,0,0" Name="Fee" VerticalAlignment="Top" Width="120" />
        <Button Content="Search" Height="23" HorizontalAlignment="Left" Margin="270,38,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click" />
        <DataGrid AutoGenerateColumns="False" Height="154" HorizontalAlignment="Left" Margin="12,79,0,0" Name="dataGrid1" VerticalAlignment="Top" Width="343" SelectionChanged="dataGrid1_SelectionChanged_1">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Id" Binding="{Binding Path=Id}"/>
                <DataGridTextColumn Header="Name" Binding="{Binding Path=Name}"/>
                <DataGridTextColumn Header="Fee" Binding="{Binding Path=Fee}"/>
                <DataGridTextColumn Header="Description" Binding="{Binding Path=Description}"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
